<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Singleton Design Test Suite" parallel="tests" thread-count="2">
    <test name="Singleton Chrome Test">
        <parameter name="browser" value="CHROME" />
        <classes>
            <class name="qa.design.singleton.DriverInvocationTest" />
        </classes>
    </test>
    <test name="Singleton Edge Test">
        <parameter name="browser" value="EDGE" />
        <classes>
            <class name="qa.design.singleton.DriverInvocationTest" />
        </classes>
    </test>
</suite> <!-- Suite -->
